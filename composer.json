{
  "name": "drupal/dropsolid_rocketship_profile",
  "description": "The installation profile for Dropsolid products.",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "Kevin Van Belle",
      "homepage": "https://www.drupal.org/u/kevinvb",
      "role": "Maintainer"
    },
    {
      "name": "Brent Gees",
      "homepage": "https://www.drupal.org/u/brentgees",
      "role": "Maintainer"
    },
    {
      "name": "Nick Vanpraet",
      "homepage": "https://www.drupal.org/u/grayle",
      "role": "Maintainer"
    }
  ],
  "require": {
    "drupal/console": "~1.0",
    "drupal/devel": "~2.0",
    "drupal/devel_php": "^1.2",
    "drupal/facets": "^1.4",
    "drupal/views_exposed_filter_blocks": "^1.1",
    "drupal/captcha": "^1.1",
    "drupal/recaptcha": "^3.0",
    "drupal/field_hidden": "^1.0",
    "drupal/reroute_email": "^1.2",
    "drupal/environment_indicator": "^3.7",
    "drupal/memcache": "^2.1",
    "drupal/dropsolid_purge": "^1.0",
    "drupal/purge": "^3.0",
    "drupal/webform_mailchimp": "^5.9",
    "drupal/twig_xdebug": "^1.2",
    "drupal/rabbit_hole": "^1.0",
    "drupal/chosen": "^2.9",
    "drupal/allowed_formats": "^1.2",
    "drupal/styleguide": "^1.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/geocoder": "^2.0",
    "drupal/geofield": "^1.0",
    "drupal/geofield_map": "~2.13",
    "drupal/address": "^1.4",
    "drupal/webform_views": "^5.0",
    "drupal/disable_language": "^1.0",
    "drupal/office_hours": "^1.3",
    "drupal/imageapi_optimize": "^2.0",
    "drupal/imageapi_optimize_binaries": "^1.0",
    "drupal/imageapi_optimize_resmushit": "^1.0",
    "drupal/rocketship_core": "^5.0",
    "drupal/rocketship_paragraphs": "^5.0",
    "drupal/r4032login": "^1.0",
    "drupal/menu_link_attributes": "^1.0",
    "drupal/lb_ux": "^1.0",
    "drupal/layout_builder_modal": "^1.1"
  },
  "support": {
    "docs": "https://drupal.org/project/dropsolid_rocketship_profile"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Support priority for needs_destruction service tag": "https://www.drupal.org/files/issues/2019-03-06/support-priority-for-need_destruction-tag.patch",
        "Toolbar's renderPlain() is incompatible with dynamic page cache": "https://www.drupal.org/files/issues/2020-04-24/2949457-53.patch",
        "Exposed term filter is not updated when terms are added, deleted, or rearranged  https://www.drupal.org/project/drupal/issues/2900248": "https://www.drupal.org/files/issues/2019-05-07/2900248-16.patch",
        "Allow states to check and uncheck checkboxes and radio buttons": "https://www.drupal.org/files/issues/drupal-994360-74-states-checkboxes-checked.patch",
        "Allow Views pages to be marked as admin page": "https://www.drupal.org/files/issues/2018-09-01/2719797-69.patch",
        "Fix datetime widget descriptions": "https://www.drupal.org/files/issues/2019-03-16/2791693-38.patch",
        "PECL YAML errors at valid YAML file which only contains comments": "https://www.drupal.org/files/issues/2018-09-30/pecl_yaml_only_comments_error-3003300-4.patch",
        "Incorrect translation status for duplicated entities. -- https://www.drupal.org/project/drupal/issues/3009016#comment-12828375": "https://www.drupal.org/files/issues/2018-10-25/3009016-6.patch",
        "Media entities should not have path alias by default - https://www.drupal.org/project/drupal/issues/3067944": "https://www.drupal.org/files/issues/2019-07-15/drupal.3067944.2.patch",
        "https://www.drupal.org/files/issues/2020-01-07/2796399-24.patch": "https://www.drupal.org/files/issues/2020-01-07/2796399-24.patch",
        "Upload via Media Library field widget does not take all upload validators in account": "https://www.drupal.org/files/issues/2019-12-11/media_library---file---upload_form_not_getting_populated_with_image_size_validators-3008292-8.7.x.patch"
      },
      "drupal/purge": {
        "Give destruct service tags priority. Depends on related core patch": "https://www.drupal.org/files/issues/2020-06-19/3008776.add-priority-to-needs_destruct.8.patch"
      },
      "drupal/facets": {
        "Hide facets when it is configured to hide the block - https://www.drupal.org/node/2984465": "https://www.drupal.org/files/issues/2019-04-08/facets-empty-block-2984465-26.patch"
      },
      "drupal/r4032login": {
        "Add cache tags to redirect": "https://www.drupal.org/files/issues/2019-03-13/2941064-20.patch",
        "https://www.drupal.org/project/r4032login/issues/3065998": "https://www.drupal.org/files/issues/2019-07-05/r4032login.3065998.add-307-temp-redirect-option.patch"
      },
      "drupal/lb_ux": {
          "Errors when using with modules that alter a section's contextual menu": "https://www.drupal.org/files/issues/2020-05-22/3106939-4.patch",
          "Show contextual cog when content preview is disabled": "https://www.drupal.org/files/issues/2020-05-22/3116402-8.patch"
      }
    }
  }
}
